<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--指定数据库名-->
    <dbname value="newsApp"></dbname>

    <!--用于指定数据库版本号-->
    <version value="11" ></version>

    <!--用于指定所有的映射模型列表,声明要配置的映射模型类，写所定义数据库表的类名路径，注意一定要使用完整的类名-->
    <list>
        <mapping class="com.example.viewnews.bean.NewsInfoBean"></mapping>
        <mapping class="com.example.viewnews.bean.UserInfo"></mapping>
        <mapping class="com.example.viewnews.bean.NewsCollectBean"></mapping>
        <mapping class="com.example.viewnews.bean.Article"></mapping>
        <mapping class="com.example.viewnews.bean.commentBean"></mapping>
        <mapping class="com.example.viewnews.bean.SubBean"></mapping>
    </list>
</litepal>